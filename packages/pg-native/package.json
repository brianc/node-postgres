{
  "name": "pg-native",
  "version": "3.5.2",
  "description": "A slightly nicer interface to Postgres over node-libpq",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./index.js",
      "default": "./index.js"
    },
    "./lib/*": {
      "import": "./lib/*",
      "require": "./lib/*",
      "default": "./lib/*"
    }
  },
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brianc/node-postgres.git"
  },
  "keywords": [
    "postgres",
    "pg",
    "libpq"
  ],
  "author": "Brian M. Carlson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianc/node-postgres/issues"
  },
  "homepage": "https://github.com/brianc/node-postgres/tree/master/packages/pg-native",
  "dependencies": {
    "libpq": "^1.9.0",
    "pg-types": "2.2.0"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "concat-stream": "^1.4.6",
    "generic-pool": "^2.1.1",
    "lodash": "^4.17.21",
    "mocha": "10.5.2",
    "node-gyp": ">=10.x",
    "okay": "^0.3.0",
    "semver": "^7.7.2"
  },
  "files": [
    "index.js",
    "lib",
    "esm"
  ]
}
