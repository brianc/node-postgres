  "bugs": {
    "url": "https://github.com/brianc/node-postgres/issues"
  },
  "homepage": "https://github.com/brianc/node-postgres/tree/master/packages/pg-connection-string",
  "devDependencies": {
    "@types/pg": "^8.12.0",
    "chai": "^6.0.1",
    "coveralls": "^3.0.4",
    "istanbul": "^0.4.5",
    "mocha": "^10.5.2",
    "nyc": "^15",
    "tsx": "^4.19.4",
    "typescript": "^4.0.3"
